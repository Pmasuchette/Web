function MostrarHora(){
  let Hora = new Date()
  console.log(`Agora são : ${Hora.toLocaleTimeString()}`)
}
MostrarHora()

function mensagem(){
  console.log('Ola, como vai?')
}
mensagem()

function cumprimentar(nome){
  console.log(`Ola ${nome}`)
}
cumprimentar('amanda')

function soma(a, b) { 
 return a + b; 
} 
// Chamando a função e armazenando o retorno em uma variável 
let resultado = soma(5, 3); 
// Exibindo o resultado no console 
console.log("O resultado da soma é:", resultado);


let pessoa = {
  nome: "Magnus",
  idade: 20,
  profissao: "Presidente"
};

pessoa.idade = 26;
pessoa["idade"] = 26;

// remover
delete pessoa.profissao;

// exibindo no console o resultado
console.log(pessoa);
// Resultado: { nome: 'Ana', idade: 26 }
